@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org/> .

ex:TheHobbit rdf:type ex:Movie ;
    ex:title "The Hobbit" ;
    ex:year "2012" ;
    ex:directs ex:PeterJackson ;
    ex:stars ex:MartinFreeman, ex:IanMcKellen .

ex:PeterJackson rdf:type ex:Director ;
    ex:name "Peter Jackson" .

ex:MartinFreeman rdf:type ex:Actor ;
    ex:name "Martin Freeman" .

ex:IanMcKellen rdf:type ex:Actor ;
    ex:name "Ian McKellen" .
